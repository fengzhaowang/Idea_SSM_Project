<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiamaliu.dao.IndexDao">
    <select id="findStudent" resultType="com.xiamaliu.entity.Student">
        select * from student where 1 = 1
    </select>
    <select id="findUserInfo" resultType="com.xiamaliu.entity.UserInfo">
        select * from user_info
    </select>
</mapper>